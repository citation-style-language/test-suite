      </if>
      <else-if type="entry-encyclopedia chapter article-journal article-newspaper article-magazine">
        <text font-style="italic" macro="us-title-short-action"/>
      </else-if>
      <else-if type="legal_case bill legislation">
        <text font-style="italic" macro="us-title-short-action"/>
