      </if>
      <else-if type="article-newspaper article-magazine">
        <text macro="gb-case-newspaper"/>
      </else-if>
      <else-if type="interview">
        <text macro="gb-government-transcript"/>
      </else-if>
      <else>
        <text macro="gb-government-report"/>
      </else>
