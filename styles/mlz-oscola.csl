          <text value="Sess."/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="us-us-government-transcript">
    <text value="[us-us-government-transcript]"/>
  </macro>
  <macro name="us-us-government-report">
    <text value="[us-us-government-report]"/>
  </macro>
  <macro name="us-title-short-action">
    <choose>
      <if variable="hereinafter">
        <text font-variant="small-caps" variable="hereinafter"/>
      </if>
      <else-if type="legal_case bill legislation">
        <choose>
          <if match="none" position="near-note">
            <text font-style="italic" form="short" variable="title"/>
          </if>
          <else-if match="none" variable="volume">
            <text font-style="italic" form="short" variable="title"/>
          </else-if>
        </choose>
      </else-if>
      <else>
        <text form="short" variable="title"/>
      </else>
    </choose>
  </macro>
  
  <macro name="us-title-short">
    <choose>
      <if match="none" variable="author">
        <text macro="us-title-short-format"/>
      </if>
      <else-if disambiguate="true">
        <text macro="us-title-short-format"/>
      </else-if>
    </choose>
  </macro>
  <macro name="us-statute-subsequent">
    <group delimiter=" ">
      <group delimiter="">
        <group delimiter=" ">
          <number variable="volume"/>
          <text variable="container-title"/>
          <number variable="number"/>
          <number variable="page"/>
          <text form="symbol" term="section"/>
          <text variable="section"/>
        </group>
        <text macro="us-point-locator-comma"/>
      </group>
      <text macro="us-point-locator"/>
      <date date-parts="year" form="text" prefix="(" suffix=")" variable="original-date"/>
    </group>
  </macro>
  <macro name="us-statute-first">
    <group delimiter=", ">
      <text variable="title"/>
      <text macro="us-statute-subsequent"/>
    </group>
  </macro>
  <macro name="us-point-locator-comma">
    <group delimiter=" ">
      <choose>
        <if match="all" variable="jurisdiction locator">
          <choose>
            <if type="bill legislation">
              <choose>
                <if match="any" variable="number page section">
                  <text value=","/>
                </if>
              </choose>
            </if>
            <else-if match="all" type="legal_case" variable="title page volume">
              <text value=","/>
            </else-if>
          </choose>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="us-point-locator">
    <choose>
      <if match="any" type="bill legislation" variable="volume">
        <choose>
          <if locator="page" match="none">
            <group delimiter=" ">
              <label form="symbol" variable="locator"/>
              <number variable="locator"/>
            </group>
          </if>
          <else>
            <number variable="locator"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  
  <macro name="us-legislation-subsequent">
    <choose>
      <if variable="container-title">
        <text macro="us-statute-subsequent"/>
      </if>
      <else>
        <text macro="us-code-subsequent"/>
      </else>
    </choose>
  </macro>
  <macro name="us-legislation-first">
    <choose>
      <if variable="container-title">
        <text macro="us-statute-first"/>
      </if>
      <else>
        <text macro="us-code-first"/>
      </else>
    </choose>
  </macro>
  <macro name="us-legislation">
    <choose>
      <if position="first">
        <text macro="us-legislation-first"/>
      </if>
      <else-if position="near-note">
        <text macro="us-legislation-subsequent"/>
      </else-if>
      <else>
        <text macro="us-legislation-first"/>
      </else>
    </choose>
  </macro>
  <macro name="us-legal-case-subsequent">
    <group delimiter=", ">
      <text font-style="italic" macro="us-title-short"/>
      <group delimiter=" ">
        <group delimiter=", ">
          <choose>
            <if variable="volume">
              <number variable="volume"/>
            </if>
            <else>
              <choose>
                <if match="none" position="near-note">
                  <group delimiter=" ">
                    <text value="No."/>
                    <number variable="number"/>
                  </group>
                </if>
              </choose>
              <group delimiter=" ">
                <text variable="archive"/>
                <text variable="archive_location"/>
              </group>
              <group delimiter=" ">
                <text value="at"/>
                <text variable="locator"/>
              </group>
            </else>
          </choose>
        </group>
        <group delimiter=", ">
          <text form="short" variable="container-title"/>
          <choose>
            <if type="article-newspaper">
              <date date-parts="year-month-day" form="text" variable="issued"/>
              <choose>
                <if match="all" variable="issued page">
                  <text value="at"/>
                </if>
              </choose>
            </if>
          </choose>
        </group>
        <choose>
          <if variable="locator">
            <text macro="us-point-locator"/>
          </if>
          <else>
            <number variable="page"/>
          </else>
        </choose>
      </group>
    </group>
  </macro>
  
  <macro name="us-legal-case-first">
    <choose>
      <if variable="title">
        <group delimiter=" ">
          <text font-style="italic" form="short" variable="title"/>
          <group delimiter=" ">
            <choose>
              <if variable="volume">
                <number variable="volume"/>
              </if>
              <else>
                <group delimiter=", ">
                  <text macro="us-slip-opinion-locator"/>
                </group>
              </else>
            </choose>
            <group delimiter=", ">
              <text form="short" variable="container-title"/>
              <group delimiter="">
                <choose>
                  <if type="article-newspaper">
                    <date date-parts="year-month-day" form="text" variable="issued"/>
                    <choose>
                      <if match="all" variable="issued page">
                        <text value=", "/>
                      </if>
                    </choose>
                  </if>
                </choose>
              </group>
            </group>
            <group delimiter=" ">
              <group delimiter="">
                <number variable="page"/>
                <text macro="us-point-locator-comma"/>
              </group>
              <text macro="us-point-locator"/>
            </group>
            <group delimiter=" " prefix="(" suffix=")">
              <text form="short" variable="subjurisdiction"/>
              <text macro="std-authority-hint"/>
              <choose>
                <if variable="volume">
                  <date date-parts="year" form="text" variable="original-date"/>
                </if>
                <else>
                  <date date-parts="year-month-day" form="text" variable="original-date"/>
                </else>
              </choose>
            </group>
          </group>
        </group>
      </if>
      <else>
        <group delimiter=", ">
          <text macro="std-authority-key"/>
          <text form="short" variable="title"/>
          <date form="text" variable="original-date"/>
          <text font-style="italic" variable="container-title"/>
          <number variable="volume"/>
          <group delimiter=" ">
            <text variable="section"/>
            <label form="short" variable="page"/>
            <number variable="page"/>
          </group>
          <number variable="number"/>
          <group delimiter=" ">
            <text variable="genre"/>
            <text font-variant="small-caps" macro="us-author"/>
          </group>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="us-legal-case">
    <choose>
      <if position="ibid-with-locator">
        <text macro="us-ibid-with-locator"/>
      </if>
      <else-if position="ibid">
        <text font-style="italic" term="ibid"/>
      </else-if>
      <else-if position="near-note">
        <text macro="us-legal-case-subsequent"/>
      </else-if>
      <else-if position="first">
        <text macro="us-legal-case-first"/>
      </else-if>
      <else>
        <text macro="us-legal-case-subsequent"/>
      </else>
    </choose>
  </macro>
  <macro name="us-ibid-with-locator">
    <choose>
      <if position="ibid-with-locator">
        <text font-style="italic" term="ibid"/>
        <group delimiter=" ">
          <choose>
            <if locator="page">
              <text value=" at"/>
            </if>
            <else>
              <text value=", "/>
              <label form="short" variable="locator"/>
            </else>
          </choose>
          <number variable="locator"/>
        </group>
      </if>
      <else-if position="ibid">
        <text font-style="italic" term="ibid"/>
      </else-if>
    </choose>
  </macro>
  
  <macro name="us-code-subsequent">
    <group delimiter=" ">
      <text font-variant="small-caps" form="short" variable="title"/>
      <group delimiter=" ">
        <label form="symbol" variable="locator"/>
        <number variable="locator"/>
      </group>
    </group>
  </macro>
  <macro name="us-code-first">
    <group delimiter=" ">
      <text font-variant="small-caps" variable="title"/>
      <group delimiter=" ">
        <label form="symbol" variable="locator"/>
        <number variable="locator"/>
      </group>
    </group>
  </macro>
  <macro name="us-bill">
    <group delimiter=" ">
      <group delimiter=", ">
        <choose>
          <if position="first">
            <text variable="title"/>
          </if>
          <else-if position="near-note">
            <text variable="title"/>
          </else-if>
        </choose>
        <group delimiter=" ">
          <choose>
            <if variable="event">
              <text form="short" variable="event"/>
            </if>
            <else>
              <text macro="std-authority-hint"/>
            </else>
          </choose>
          <number variable="number"/>
        </group>
        <group delimiter=" ">
          <number form="ordinal" variable="collection-number"/>
          <text macro="std-authority-parent"/>
        </group>
        <choose>
          <if match="any" variable="subjurisdiction">
            <text macro="us-session-number"/>
          </if>
        </choose>
      </group>
      <text variable="section"/>
      <group delimiter=" " prefix="(" suffix=")">
        <choose>
          <if match="none" variable="subjurisdiction">
            <text macro="us-session-number"/>
          </if>
          <else>
            <text form="short" variable="subjurisdiction"/>
          </else>
        </choose>
        <date date-parts="year" form="text" variable="original-date"/>
      </group>
    </group>
  </macro>
  <macro name="us-author">
    <choose>
      <if type="interview">
        <names variable="author">
          <name and="symbol" delimiter-precedes-last="never" initialize="false" initialize-with="."/>
          <institution use-first="1" use-last="1"/>
        </names>
      </if>
      <else>
        <names variable="author">
          <name and="symbol" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
          <substitute>
            <names variable="container-author"/>
            <names variable="composer"/>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="jurisdictions">
