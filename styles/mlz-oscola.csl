      <group delimiter=" ">
        <text variable="archive"/>
        <text variable="archive_location"/>
      </group>
      <group delimiter=" ">
        <choose>
          <if variable="archive">
            <text value=""/>
          </if>
          <else>
            <text value="slip op. at"/>
