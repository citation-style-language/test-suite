        <text value=", "/>
      </else>
    </choose>
    <group delimiter=", ">
      <group prefix="(" suffix=")" delimiter=", ">
        <text variable="publisher" form="short"/>
        <text macro="foreign-editor"/>
        <group delimiter=" ">
