      <if position="near-note">
        <text macro="foreign-legal-case-subsequent"/>
      </if>
      <else-if position="ibid">
        <text macro="foreign-ibid"/>
      </else-if>
      <else-if position="first">
        <text macro="foreign-legal-case-first"/>
      </else-if>
      <else>
        <text macro="foreign-legal-case-first"/>
      </else>
    </choose>
  </macro>
  <macro name="foreign-legal-case-first">
    <choose>
      <if variable="title">
        <group delimiter=", ">
          <text variable="title"/>
          <group delimiter=" ">
            <choose>
              <if variable="issue">
                <number variable="volume" prefix="[" suffix="]"/>
                <number variable="issue"/>
              </if>
              <else>
                <number variable="volume"/>
              </else>
            </choose>
            <text variable="container-title" form="short"/>
            <group>
              <number variable="page"/>
              <text macro="foreign-point-locator-comma"/>
            </group>
            <group prefix="(" suffix=")" delimiter=" ">
              <names variable="authority"/>
              <date variable="issued">
                <date-part name="year"/>
              </date>
            </group>
          </group>
        </group>
      </if>
      <else>
        <group delimiter=", ">
          <names variable="authority"/>
          <text variable="title" form="short"/>
          <date variable="issued" form="text"/>
          <text variable="container-title" font-style="italic" strip-periods="true"/>
          <number variable="volume"/>
          <group delimiter=" ">
            <text variable="section"/>
            <label variable="page" form="short"/>
            <number variable="page"/>
          </group>
          <number variable="number"/>
          <group delimiter=" ">
            <text variable="genre"/>
            <text macro="foreign-author" font-variant="small-caps"/>
          </group>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="foreign-legal-case-subsequent">
    <group delimiter=", ">
      <text macro="foreign-short-title"/>
      <group delimiter=" ">
        <choose>
          <if variable="issue">
            <number variable="volume" prefix="[" suffix="]"/>
            <number variable="issue"/>
          </if>
          <else>
            <number variable="volume"/>
          </else>
        </choose>
        <text variable="container-title" form="short"/>
        <choose>
          <if variable="locator">
            <text macro="foreign-point-locator-base"/>
          </if>
          <else>
            <number variable="page"/>
          </else>
        </choose>
      </group>
    </group>
  </macro>
  <macro name="foreign-legal-case-bibliography">
    <group delimiter=", ">
      <text variable="title"/>
      <group delimiter=" ">
        <choose>
