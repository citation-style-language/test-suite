              </choose>
            </else-if>
            <else-if type="patent">
              <text label-form="short" macro="std-locator-paragraph-bracket-at"/>
            </else-if>
            <else-if match="any" type="bill gazette legislation">
              <text label-form="symbol" macro="std-locator-non-page-label-at"/>
